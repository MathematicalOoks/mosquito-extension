{
    "name": "Mosquito",
    "version": "1.0",
    "manifest_version": 3,
    "description": "An extension to display a mosquito or fly on the screen.",
    "icons": {
        "16": "icons/16x16icon.png",
        "32": "icons/32x32icon.png",
        "48": "icons/48x48icon.png",
        "128": "icons/128x128icon.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "web_accessible_resources": [
        {
          "resources": ["icons/*.png"],
          "matches": ["<all_urls>"]
        }
    ]
}